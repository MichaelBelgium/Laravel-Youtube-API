{
    "name": "michaelbelgium/laravel-youtube-api",
    "description": "Add a youtube api to your Laravel instance",
    "keywords": [
        "michaelbelgium",
        "laravel-youtube-api",
        "youtube-api",
        "laravel",
        "youtube"
    ],
    "homepage": "https://github.com/michaelbelgium/laravel-youtube-api",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Michael V.",
            "email": "michael@michaelbelgium.me",
            "role": "Developer"
        }
    ],
    "require": {
        "google/apiclient": "^2.4",
        "illuminate/support": "^8.0|^9.0",
        "norkunas/youtube-dl-php": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MichaelBelgium\\YoutubeAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelBelgium\\YoutubeAPI\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelBelgium\\YoutubeAPI\\YoutubeAPIServiceProvider"
            ]
        }
    }
}
